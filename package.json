{
  "homepage": "https://github.com/marcusasplund/hyperapp-todo-parcel#readme",
  "license": "MIT",
  "main": "src/index.js",
  "name": "hyperapp-todo-parcel",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/marcusasplund/hyperapp-todo-parcel.git"
  },
  "scripts": {
    "start": "standard && parcel src/index.html -p 3000 --open",
    "build": "rm -rf /release && standard && parcel build src/index.html -d build --public-url ./"
  },
  "version": "0.0.1",
  "author": "Marcus Asplund <marcus@greatname.se> (@marcusasplund)",
  "bugs": {
    "url": "https://github.com/marcusasplund/hyperapp-todo-parcel/issues"
  },
  "cache": {
    "inDev": true,
    "swDest": "service-worker.js"
  },  
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/preset-env": "7.1.5",
    "babel-eslint": "^10.0.1",
    "babel-preset-env": "^1.7.0",
    "parcel-bundler": "^1.10.3",
    "parcel-plugin-sw-cache": "^0.2.6",
    "standard": "*",
    "node-sass": "^4.10.0"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "dist/",
      "release/",
      "build/"
    ]
  },
  "dependencies": {
    "hyperapp": "^1.2.9"
  }
}
